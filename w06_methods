
void eyething ( int xc, int yc, int fat, int tall) {
  fill (0);
  ellipse (xc, yc, fat, tall);
  fill (255);
  ellipse (xc, yc, tall, tall);
  fill (#EADF02);  
}

void setup () {
size (400, 400);
background (#EA0202);
eyething (200, 200, 400, 200);
eyething (75, 75, 75, 25);
eyething (325, 75, 75, 25);

}
